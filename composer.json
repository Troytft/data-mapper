{
    "name" : "troytft/data-mapper-bundle",
    "description": "Allow mapping data to models",
    "type" : "symfony-bundle",
    "license" : "MIT",
    "require" : {
        "php": ">=7.2.5",
        "symfony/validator": "^4.4 || ^5.2",
        "doctrine/orm": "^2.5",
        "symfony/framework-bundle": "^4.4 || ^5.2",
        "symfony/yaml": "^4.4 || ^5.2"
    },
    "autoload" : {
        "psr-4" : {
            "Troytft\\DataMapperBundle\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Troytft\\DataMapperBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "phpstan/phpstan": "^0.12"
    }
}
