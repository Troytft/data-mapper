{
    "name" : "troytft/data-mapper-bundle",
    "description": "Allow mapping data to models",
    "version" : "2.5.7",
    "type" : "symfony-bundle",
    "license" : "MIT",
    "require" : {
        "php": "^7.0",
        "symfony/validator": "~3.3",
        "doctrine/orm": "~2.5",
        "symfony/dependency-injection": "~3.3",
        "symfony/http-kernel": "~3.3",
        "symfony/yaml": "~3.3",
        "symfony/config": "~3.3"
    },
    "autoload" : {
        "psr-4" : {
            "Troytft\\DataMapperBundle\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Troytft\\DataMapperBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "phpstan/phpstan": "^0.9.2"
    }
}
